{
    "name": "beepnl/beep",
    "version": "2.1.0",
    "description": "BEEP digital hive monitor",
    "keywords": ["Beep", "Beekeeping", "apiary", "hive", "app", "measurement system"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.1.3",
        "fideloper/proxy": "^4.0",
        "barryvdh/laravel-cors": "0.11.*",
        "ellipsesynergie/api-response": "0.12.*",
        "fightbulc/moment": "*",
        "iatstuti/laravel-cascade-soft-deletes": "^1.4",
        "influxdb/influxdb-php": "^1.14",
        "intervention/image": "^2.4.2",
        "kalnoy/nestedset": "^4.3",
        "laravel/framework": "5.7.*",
        "laravel/tinker": "~1.0",
        "laravelcollective/html": "^5.7",
        "maatwebsite/excel": "^2.1",
        "phpoffice/phpspreadsheet": "^1.2",
        "mcamara/laravel-localization": "1.3.*",
        "predis/predis": "^1.1",
        "pdffiller/laravel-influx-provider": "^1.6",
        "zizaco/entrust": "5.2.x-dev"
    },
    "require-dev": {
        "appzcoder/crud-generator": "^3.0",
        "beyondcode/laravel-dump-server": "^1.0",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "^7.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
